{
    "$schema": "./schema/Integration.json",
    "firmatas": [
        {
            "name": "5v.power.controller",
            "desc": "Control the power for rpi/opi or other 5v devices",
            "board": "stm32f103c8t6-blue",
            "dial": "serial:///dev/ttyUSB0?baud=115200&timeout=5s",
            "wiring": [
                {
                    "name":"OE",
                    "from": {
                        "name": "3v3-5v OE",
                        "gpioName": "PB0"
                    },
                    "to": {
                        "device": {
                            "name": "3v3-5v OE",
                            "first": "OE"
                        }
                    },
                    "autoHigh": true
                },
                {
                    "name": "signals",
                    "desc": "use all analog pins as signals out pins",
                    "from": {
                        "name": "analog-pins",
                        "desc": "signal pins",
                        "gpioName": "PA0",
                        "lastGpioName": "PA7"
                    },
                    "to": {
                        "device": {
                            "name": "3v3-5v",
                            "desc": "VCCA",
                            "first": "A1",
                            "last": "A8"
                        }
                    }
                }
            ]
        }
    ],
    "devices": [
        {
            "name": "3v3-5v",
            "desc": "VCCA: 1.2V-3.6V, VCCB: 1.65V-5.5V",
            "module": "TXS0108E",
            "wiring": [
                {
                    "name": "to.relay.5v",
                    "from": {
                        "name": "VCCB",
                        "first": "B1",
                        "last": "B8"
                    },
                    "to": {
                        "device": {
                            "name": "relay.5v",
                            "first": "IN1",
                            "last": "IN8"
                        }
                    }
                }
            ]
        },
        {
            "name": "relay.5v",
            "desc": "BESTEP, 5V, Low Level Trigger",
            "module": "8.relay.5v"
        }
    ]
}